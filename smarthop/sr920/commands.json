  "definitions": {
    "config_value": [
      {
        "type": "select:config_id",
        "cases": [
          {
            "case": ["TX_POWER"],
            "parameters": [
              {
                "name": "value",
                "description": "送信出力",
                "type": "enum:SR920TxPower",
                "length": 1
              }
            ]
          },
          {
            "case": ["MAC_ADDRESS"],
            "parameters": [
              {
                "name": "value",
                "description": "MACアドレス",
                "type": "hex",
                "length": 8,
                "byteorder": "little"
              }
            ]
          },
          {
            "case": ["ASYNC_FALLBACK_COUNT"],
            "parameters": [
              {
                "name": "reserved",
                "description": "（固定値）",
                "type": "hex",
                "length": 4,
                "value": "0108050a"
              },
              {
                "name": "value",
                "description": "非同期通信切替回数",
                "type": "int",
                "length": 1
              },
              {
                "name": "reserved",
                "description": "（固定値）",
                "type": "hex",
                "length": 1,
                "value": "04"
              }
            ]
          },
          {
            "case": ["LED"],
            "parameters": [
              {
                "name": "value",
                "description": "LED設定",
                "type": "bool",
                "length": 1
              }
            ]
          },
          {
            "case": ["DUMMY_SIZE"],
            "parameters": [
              {
                "name": "value",
                "description": "UART通信におけるDummy部サイズ",
                "type": "int",
                "length": 2,
                "byteorder": "little"
              }
            ]
          },
          {
            "case": ["PARENT_SELECTION_MODE"],
            "parameters": [
              {
                "name": "reserved",
                "description": "（固定値）",
                "type": "hex",
                "length": 4,
                "value": "03000001"
              },
              {
                "name": "value",
